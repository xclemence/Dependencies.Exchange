<UserControl x:Class="Dependencies.Exchange.Graph.Views.GraphSettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <GroupBox Header="Graph Settings">
        <DockPanel Margin="10">
            <TextBlock Text="Uri: " VerticalAlignment="Center"/>
            <TextBox Text="{Binding Uri}" />
        </DockPanel>
    </GroupBox>
</UserControl>
